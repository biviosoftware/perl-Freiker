Type();
[
    {
	method => 'from_literal',
	compute_return => sub {
	    my(undef, $actual) = @_;
	    my($x) = $actual->[0];
	    return $x ? [
		$x->get('zip'), $x->get('freiker_code'), $x->as_string,
	    ] : $actual;
	},
    } => [
	'' => UNDEF(),
	'000000004652075BCD153039'
	    => ['123456789', '12345', '000000004652075BCD153039'],
    ],
];
