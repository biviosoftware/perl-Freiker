Request();
do('./SetupRides.PL');
[
    [req()] => [
	{
	    method => 'validate_login',
	    compute_return => sub {
		my($case, $actual) = @_;
		my($realm) = @$actual;
		return [$realm ? $realm->get('name')
		    : $case->get('object')->get_field_error('login')->get_name];
	    },
	} => [
	    adm => 'adm',
	    1236 => 'OFFLINE_USER',
	    1234 => 'parent',
	],
    ],
];
