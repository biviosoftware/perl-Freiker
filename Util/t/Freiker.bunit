ShellUtil();
req()->set_realm_and_user(qw(parent child0));
my($rides) = model('FreikerRideList', {
    parent_id => req('auth_user_id'),
    auth_id => req('auth_id'),
})->map_rows(sub {class('Type.Date')->to_file_name(shift->get('Ride.ride_date'))});
[
    [qw(missing_rides 1234)] => sub {
	my(undef, $actual) = @_;
	foreach my $d (@$rides) {
	    assert_equals([], [$actual->[0] =~ /($d)/]);
	}
	return 1;
    },
];
